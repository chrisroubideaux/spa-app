{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "./**/*.ts" // Adjust this path based on the structure of your TypeScript files
    ],
    "exclude": [
      "node_modules",
      "dist" // Exclude the output directory to prevent recompiling generated JavaScript files
    ]
  }
  
  